   "source": [
    "x = int(input(\"عدد را وارد کنید :\"))\n",
    "i=2 \n",
    "y=x//2\n",
    "while i<=y:\n",
    "    if not x%i :\n",
    "        print(x, \"یک عدد اول نیست.\")\n",
    "        break\n",
    "    i+=1\n",
    "else :\n",
    "    print(x, \"یک عدد اول است.\")"
   ]
